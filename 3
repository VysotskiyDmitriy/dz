/*Создайте массив из минимум 100 случайных целых чисел. Переберите массив и сгенерируйте на его основе новый, который содержит только уникальные (неповторяющиеся) значения исходного массива. Решите, используя только цикл for.*/

let mas = [];
let mas2 = [];

for(let i = 0; i<100; i++){
  let num = Math.floor(Math.random()*100);
  mas[i] = num;
  if (mas2.includes(num) == false) {
    mas2.push(num);
  }
}
console.log(mas);
console.log(mas2);
